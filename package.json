{
  "name": "nevergreen",
  "version": "0.0.0",
  "description": "A build monitor with attitude",
  "private": true,
  "author": "Build Canaries",
  "homepage": "https://nevergreen.io",
  "license": "EPL-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/build-canaries/nevergreen.git"
  },
  "bugs": {
    "url": "https://github.com/build-canaries/nevergreen/issues"
  },
  "scripts": {
    "clean": "rm -rf resources/public",
    "build": "NODE_ENV=dev webpack --progress",
    "build-prod": "NODE_ENV=production webpack -p",
    "test": "NODE_ENV=dev mocha --require ./test/client/Setup.js --require babel-register -R mocha-circleci-reporter --reporter-options mochaFile=./target/client/test-results.xml --recursive test/client",
    "coverage": "nyc --reporter=html npm test",
    "lint": "npm run lintJs && npm run lintCss",
    "lintJs": "eslint ./src/client ./test/client",
    "lintCss": "stylelint src/client/*.scss src/client/**/*.scss src/client/**/**/*.scss src/client/**/**/**/*.scss --syntax scss",
    "watchJs": "watch 'npm run clean && npm run build' src/client",
    "watchTest": "watch 'npm run test' src/client test/client",
    "watchLint": "watch 'npm run lint' src/client test/client",
    "ci-stub-server": "node ci_stub_server/server"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "bourbon-neat": "2.0.0",
    "classnames": "2.2.5",
    "clipboard": "1.7.1",
    "date-fns": "1.29.0",
    "history": "4.7.2",
    "immutable": "3.8.2",
    "is-my-json-valid": "2.17.1",
    "localforage": "1.5.6",
    "lodash": "4.17.5",
    "modularscale-sass": "3.0.5",
    "mousetrap": "1.6.1",
    "normalize.css": "7.0.0",
    "project-name-generator": "2.1.4",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-immutable": "4.0.0",
    "redux-thunk": "2.2.0",
    "semver": "5.5.0",
    "superagent": "3.8.2"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "basic-auth-connect": "1.0.0",
    "chai": "4.1.2",
    "chai-enzyme": "1.0.0-beta.0",
    "chai-immutable": "1.6.0",
    "copy-webpack-plugin": "4.3.1",
    "css-loader": "0.28.9",
    "dirty-chai": "2.0.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.17.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-react": "7.6.1",
    "express": "4.16.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "html-webpack-plugin": "2.30.1",
    "ignore-styles": "5.0.1",
    "img-loader": "2.0.1",
    "jsdom": "11.6.2",
    "json-loader": "0.5.7",
    "mocha": "5.0.0",
    "mocha-circleci-reporter": "0.0.2",
    "node-sass": "4.7.2",
    "nyc": "11.4.1",
    "optimize-css-assets-webpack-plugin": "3.2.0",
    "postcss-loader": "2.1.0",
    "proxyquire": "1.8.0",
    "raw-loader": "0.5.1",
    "react-addons-test-utils": "15.6.2",
    "resolve-url-loader": "2.2.1",
    "sass-loader": "6.0.6",
    "sinon": "4.2.2",
    "sinon-chai": "2.14.0",
    "style-loader": "0.20.1",
    "stylelint": "8.4.0",
    "stylelint-config-standard": "18.0.0",
    "stylelint-order": "0.8.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "watch": "1.0.2",
    "webpack": "3.10.0",
    "webpack-manifest-plugin": "1.3.2"
  }
}
