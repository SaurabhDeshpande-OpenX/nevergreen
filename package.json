{
  "name": "nevergreen",
  "version": "1.0.0",
  "versionName": "Lapis Lazuli",
  "versionColour": "#26619C",
  "versionMeta": "0",
  "commitHash": "local",
  "description": "A build monitor with attitude",
  "main": "main.js",
  "scripts": {
    "clean": "rimraf resources/public",
    "build": "NODE_ENV=dev webpack --progress",
    "build-prod": "NODE_ENV=production webpack -p",
    "test": "NODE_ENV=dev mocha --require ./test/client/CreateDom.js -R mocha-circleci-reporter --reporter-options mochaFile=./target/client/test-results.xml --compilers js:babel-core/register --recursive test/client",
    "lint": "npm run lintJs && npm run lintCss",
    "lintJs": "eslint ./src/client ./test/client",
    "lintCss": "stylelint src/client/*.scss src/client/**/*.scss src/client/**/**/*.scss src/client/**/**/**/*.scss --syntax scss",
    "watchJs": "watch 'npm run clean && npm run build' src/client",
    "watchTest": "watch 'npm run test' src/client test/client",
    "watchLint": "watch 'npm run lint' src/client test/client",
    "ci-stub-server": "node ci_stub_server/server"
  },
