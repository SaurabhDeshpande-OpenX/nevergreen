{
  "name": "nevergreen",
  "version": "1.0.0",
  "versionName": "Lapis Lazuli",
  "versionColour": "#26619C",
  "versionMeta": "0",
  "commitHash": "local",
  "description": "A build monitor with attitude",
  "main": "main.js",
  "config": {
    "domain": "http://localhost:5000"
  },
  "scripts": {
    "clean": "rimraf resources/public",
    "build": "NODE_ENV=dev webpack --progress",
    "build-prod": "NODE_ENV=production webpack -p",
    "test": "NODE_ENV=dev mocha --require ./test/client/CreateDom.js -R mocha-circleci-reporter --reporter-options mochaFile=./target/client/test-results.xml --compilers js:babel-core/register --recursive test/client",
    "lint": "npm run lintJs && npm run lintCss",
    "lintJs": "eslint ./src/client ./test/client",
    "lintCss": "stylelint src/client/*.scss src/client/**/*.scss src/client/**/**/*.scss src/client/**/**/**/*.scss --syntax scss",
    "watchJs": "watch 'npm run clean && npm run build' src/client",
    "watchTest": "watch 'npm run test' src/client test/client",
    "watchLint": "watch 'npm run lint' src/client test/client",
    "ci-stub-server": "node ci_stub_server/server",
    "pa11y": "pa11y --ignore 'notice' $npm_package_config_domain"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "bourbon-neat": "2.0.0",
    "classnames": "2.2.5",
    "clipboard": "1.7.1",
    "immutable": "3.8.1",
    "is-my-json-valid": "2.16.0",
    "localforage": "1.5.0",
    "lodash": "4.17.4",
    "modularscale-sass": "3.0.2",
    "moment": "2.18.1",
    "mousetrap": "1.6.1",
    "normalize.css": "6.0.0",
    "project-name-generator": "2.1.3",
    "prop-types": "15.5.10",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "5.0.5",
    "react-router": "3.0.5",
    "redux": "3.6.0",
    "redux-immutable": "4.0.0",
    "redux-thunk": "2.2.0",
    "semver": "5.3.0",
    "superagent": "3.5.2"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-preset-env": "1.5.1",
    "babel-preset-react": "6.24.1",
    "basic-auth-connect": "1.0.0",
    "chai": "4.0.1",
    "chai-enzyme": "0.7.1",
    "chai-immutable": "1.6.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.28.4",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-plugin-babel": "4.1.1",
    "eslint-plugin-react": "7.0.1",
    "express": "4.15.3",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.28.0",
    "ignore-styles": "5.0.1",
    "img-loader": "2.0.0",
    "jsdom": "11.0.0",
    "json-loader": "0.5.4",
    "mocha": "3.4.2",
    "mocha-circleci-reporter": "0.0.2",
    "node-sass": "4.5.3",
    "pa11y": "4.10.0",
    "postcss-loader": "2.0.5",
    "proxyquire": "1.8.0",
    "react-addons-test-utils": "15.5.1",
    "resolve-url-loader": "2.0.2",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.5",
    "sinon": "2.3.2",
    "sinon-chai": "2.10.0",
    "style-loader": "0.18.1",
    "stylelint": "7.10.1",
    "stylelint-config-standard": "16.0.0",
    "url-loader": "0.5.8",
    "watch": "1.0.2",
    "webpack": "2.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/build-canaries/nevergreen.git"
  },
  "author": "Build Canaries",
  "license": "EPL-1.0",
  "bugs": {
    "url": "https://github.com/build-canaries/nevergreen/issues"
  }
}
