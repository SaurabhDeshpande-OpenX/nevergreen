{
  "name": "nevergreen",
  "version": "0.0.0",
  "versionName": "Dev Build",
  "versionColour": "#7E7E7E",
  "commitHash": "dev",
  "description": "A build monitor with attitude",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "main": "main.js",
  "scripts": {
    "clean": "rimraf resources/public",
    "build": "NODE_ENV=production webpack -p",
    "test": "NODE_ENV=dev mocha --compilers js:babel-core/register --recursive test/client",
    "lint": "npm run lintJs && npm run lintCss",
    "lintJs": "eslint ./src/client ./test/client",
    "lintCss": "stylelint src/client/*.scss src/client/**/*.scss src/client/**/**/*.scss --syntax scss",
    "watchJs": "watch 'npm run build' src/client",
    "watchTest": "watch 'npm run test' src/client test/client",
    "watchLint": "watch 'npm run lint' src/client test/client",
    "fake-server": "node test/test-server"
  },
  "dependencies": {
    "clipboard": "1.5.12",
    "flux": "2.1.1",
    "jquery": "3.0.0",
    "localforage": "1.4.2",
    "lodash": "4.13.1",
    "moment": "2.13.0",
    "mousetrap": "1.6.0",
    "node-uuid": "1.4.7",
    "normalize.css": "4.1.1",
    "project-name-generator": "2.1.2",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "react-router": "2.4.1",
    "scale-text": "0.2.2",
    "semver": "5.1.0",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-core": "6.9.1",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "basic-auth-connect": "1.0.0",
    "chai": "3.5.0",
    "chromedriver": "2.19.0",
    "css-loader": "0.23.1",
    "eslint": "2.13.0",
    "eslint-plugin-babel": "3.2.0",
    "eslint-plugin-react": "5.2.2",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "2.21.0",
    "ignore-styles": "4.0.0",
    "img-loader": "1.2.2",
    "json-loader": "0.5.4",
    "mocha": "2.5.3",
    "node-sass": "3.7.0",
    "postcss-loader": "0.9.1",
    "proxyquire": "1.7.9",
    "react-addons-test-utils": "15.1.0",
    "resolve-url-loader": "1.4.3",
    "rimraf": "2.5.2",
    "sass-loader": "3.2.0",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "sinon-stub-promise": "2.0.0",
    "style-loader": "0.13.1",
    "stylelint": "6.6.0",
    "stylelint-config-standard": "9.0.0",
    "url-loader": "0.5.7",
    "watch": "0.18.0",
    "webpack": "1.13.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/build-canaries/nevergreen.git"
  },
  "author": "Build Canaries",
  "license": "EPL-1.0",
  "bugs": {
    "url": "https://github.com/build-canaries/nevergreen/issues"
  }
}
